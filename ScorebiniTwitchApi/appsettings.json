{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8288"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Twitch": {
    "AppClientId": "<insert_client_id>",
    "AppClientSecret": "<insert_client_secret>",
    "TokenValidateIntervalMinutes": 50
  },
  "ConnectionStrings": {
    "AppDb": "DataSource=twitch_tokens.db;Cache=Shared"
  },
  "AllowedHosts": "*"
}
